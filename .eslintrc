{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "env": {
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "angular": false,
    "module": false,
    "inject": false,
    "ionic": false,
    "cordova": false,
    "_": false,
    "google": false,
    "parsePlugin": false,
    "moment": false,
    "facebookConnectPlugin": false,
    "StatusBar": false,
    "Camera": false
  },
  "rules": {
    "camelcase": [2, {"properties": "never"}],
    "no-console": 0,
    "angular/window-service": 0,
    "angular/no-service-method": 0
  }
}
